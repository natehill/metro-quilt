extends layout

block content
  //-h1= title
  //-p Welcome to #{title}
  div(class='container-fluid')
    div(class='row')
      div(class='col-12 control-panel')
        div(class='row')
          div(id='logo')
          div(class='thumb-back academic')
            | academic <br /> libraries
          div(id='academic-libraries' class='thumbnail orange')
          div(class='thumb-back special')
            | special <br /> libraries
          div(id='special-libraries' class='thumbnail magenta')
          div(class='thumb-back hospital')
            | hospital <br /> libraries
          div(id='hospital-libraries' class='thumbnail lavender')
          div(class='thumb-back public')
            | public <br /> libraries
          div(id='public-libraries' class='thumbnail red')
          div(class='thumb-back archives')
            | archives
          div(id='archives' class='thumbnail purple')
          div(class='thumb-back museum')
            | museum <br /> libraries
          div(id='museum-libraries' class='thumbnail blue')
          div(class='thumb-back historical')
            | historical <br /> societies
          div(id='historical-societies' class='thumbnail darkblue')
          div(class='thumb-back others')
            | others
          div(id='other' class='thumbnail yellow')
      div(class='col-12 give-a-little')
        div(id='target')
